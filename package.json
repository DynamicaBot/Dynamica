{
  "name": "dynamicabot",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^0.8.2",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@prisma/client": "^3.6.0",
    "chalk": "4.1.2",
    "discord-api-types": "^0.25.0",
    "discord.js": "^13.3.1",
